/* page */

.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page-toast {
  position: fixed;
  left: 0;
  top: 0;
  background-color: #e64340;
  color: #fff;
  padding: 0 32rpx;
  width: 100%;
  font-size: 24rpx;
  line-height: 3.2;
  z-index: 9999;
}

.page-head {
  padding: 60rpx 50rpx 80rpx;
  text-align: center;
}

.page-head-title {
  display: inline-block;
  padding: 0 40rpx 20rpx 40rpx;
  font-size: 32rpx;
  color: #bebebe;
}

.page-head-line {
  margin: 0 auto;
  width: 150rpx;
  height: 2rpx;
  background-color: #d8d8d8;
}

.page-head-desc {
  padding-top: 20rpx;
  color: #9b9b9b;
  font-size: 32rpx;
}

.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}

.page-body-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.page-body-wording {
  text-align: center;
  padding: 200rpx 100rpx;
}

.page-body-info {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  width: 100%;
  padding: 50rpx 0 150rpx 0;
}

.page-body-title {
  margin-bottom: 100rpx;
  font-size: 32rpx;
}

.page-body-text {
  font-size: 30rpx;
  line-height: 26px;
  color: #ccc;
}

.page-body-text-small {
  font-size: 24rpx;
  color: #000;
  margin-bottom: 100rpx;
}

.page-foot {
  margin: 100rpx 0 30rpx 0;
  text-align: center;
  color: #1aad19;
  font-size: 0;
}

.ex-icon-foot {
  width: 152rpx;
  height: 23rpx;
}

.page-section {
  width: 100%;
  margin-bottom: 32rpx;
  position: relative;
}

.page-section_center {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.page-section:last-child {
  margin-bottom: 0;
}

.page-section-gap {
  box-sizing: border-box;
  padding: 0 30rpx;
}

.page-section-spacing {
  box-sizing: border-box;
  padding: 0 80rpx;
}

.page-section-title {
  font-size: 28rpx;
  color: #999;
  margin-bottom: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.page-section-gap .page-section-title {
  padding-left: 0;
  padding-right: 0;
}

/*****animate******/

.slide-bottom {
  animation: slide-bottom 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;
}

.slide-up {
  animation: slide-bottom 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) 3s both;
}

@keyframes slide-bottom {
  0% {
    transform: translateY(-100rpx);
  }

  100% {
    transform: translateY(0);
  }
}

@keyframes slide-up {
  0% {
    transform: translateY(0);
  }

  100% {
    transform: translateY(-100rpx);
  }
}

.shake-horizontal {
  animation: shake-horizontal 0.8s cubic-bezier(0.455, 0.030, 0.515, 0.955) both;
}

@keyframes shake-horizontal {
  0%, 100% {
    transform: translateX(0);
  }

  10%, 30%, 50%, 70% {
    transform: translateX(-10px);
  }

  20%, 40%, 60% {
    transform: translateX(10px);
  }

  80% {
    transform: translateX(8px);
  }

  90% {
    transform: translateX(-8px);
  }
}

.card-in {
  animation: card-in 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;
  display: block;
}

@keyframes card-in {
  0% {
    transform: translateX(1000px);
    opacity: 0;
  }

  90% {
    transform: translateX(-10px);
    opacity: 0.9;
  }

  100% {
    transform: translateX(0);
    opacity: 1;
  }
}

.card-up {
  animation: card-up 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;
  display: block;
}

@keyframes card-up {
  0% {
    transform: translateY(1000px);
    opacity: 0;
  }

  90% {
    transform: translateY(-10px);
    opacity: 0.9;
  }

  100% {
    transform: translateY(0);
    opacity: 1;
  }
}

.card-down {
  animation: card-down 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;
  display: block;
}

@keyframes card-down {
  0% {
    transform: translateY(0px);
    opacity: 1;
  }

  100% {
    transform: translateY(1000px);
    opacity: 0;
  }
}

.scale-up-center {
  animation: scale-up-center 0.2s cubic-bezier(0.47, 0.000, 0.745, 0.715) both;
}

@keyframes scale-up-center {
  0% {
    opacity: 0;
    transform: scale(0.5);
  }

  90% {
    opacity: 0.9;
    transform: scale(1.3);
  }

  100% {
    opacity: 1;
    transform: scale(1);
  }
}

.scale-up-hor-left {
  animation: scale-up-hor-left 0.4s cubic-bezier(0.39, 0.575, 0.565, 1.000) both;
}

.slide-left {
  animation: slide-left 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94) both;
}

@keyframes scale-up-hor-left {
  0% {
    transform: scaleX(0.4);
    transform-origin: 0 0;
  }

  100% {
    transform: scaleX(1);
    transform-origin: 0 0;
  }
}

@keyframes slide-left {
  0% {
    transform: translateX(0);
  }

  100% {
    transform: translateX(-100px);
  }
}
