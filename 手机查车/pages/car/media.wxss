page {
  background-color: #eeeff4;
}

.fr {
  float: right;
}

.history-card {
  width: 95%;
  background-color: #fff;
  border-radius: 4rpx;
  margin: 5px 0;
  padding: 2.5%;
  box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.05);
  display: inline-block;
}

.history-card .icon-pin {
  width: 14px;
  height: 16px;
}

.photo-list {
  /* display: flex; */
  margin: 5px auto;
}

.photo-list > view {
  /* flex: 1; */
  width: calc(50% - 4px);
  margin: 2px;
  display: inline-block;
}

.photo-list > view image {
  width: 100%;
  border-radius: 4px;
}

.history-card .timer {
  color: #acb0ba;
}

.history-card .fr {
  font-weight: 700;
}
