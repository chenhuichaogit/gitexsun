map {
  width: 100%;
  height: 45%;
}

.layout-alerm-topic {
  width: 100%;
  height: 120px;
  background-color: #fff;
}

.layout-alerm-list {
  width: 100%;
  height: calc(55% - 120px);
  background-color: #fff;
}

.alerm-list-card {
  width: 100%;
  display: flex;
  position: relative;
}

.alerm-list-card  .icon {
  width: 15%;
  text-align: center;
  background: url("data:image/png;base64,") 50% 0 repeat-y;
}

.alerm-list-card  .icon .icon-i {
  margin: 0 auto;
  display: block;
  font-size: 12px;
  font-weight: 700;
  color: #fff;
  width: 32px;
  height: 32px;
  line-height: 28px;
  margin-top: 10px;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
}

.alerm-list-card  .topic-icon .icon-i {
  margin: 0 auto;
  display: block;
  font-size: 16px;
  font-weight: 700;
  color: #fff;
  width: 44px;
  height: 44px;
  line-height: 44px;
  margin-top: 10px;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
}

.alerm-list-card  .info {
  width: calc(85% - 20px);
  padding: 10px;
}

.alerm-list-card image {
  width: 14px;
  height: 18px;
}

.alerm-list-card .title {
  font-size: 14px;
  font-weight: 700;
}

.alerm-list-card .timer {
  color: #acb0ba;
}

.alerm-list-card .seconds {
  position: absolute;
  right: 15px;
  top: 5px;
  color: #2ea2fd;
}
